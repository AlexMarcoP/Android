<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dsic.online.files">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".LauncherActivity"
            android:label="Ficheros">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Files"
            android:label="@string/title_activity_files" />
        <activity
            android:name=".SDFiles"
            android:label="@string/title_activity_sdfiles" />
        <activity
            android:name=".XMLFiles"
            android:label="@string/title_activity_xmlfiles" />
    </application>

</manifest>
